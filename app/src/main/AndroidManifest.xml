        <activity
            android:name=".OutcomeActivity"
            android:exported="false" />
        <activity
            android:name=".DetailFlowActivity"
            android:exported="false" />
        <activity
            android:name=".PengaturanActivity"
            android:exported="false" />
    </application>

</manifest>